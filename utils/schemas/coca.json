{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Coca",
  "description": "Schema for the coca object",
  "type": "object",
  "properties": {
    "passaggi": {
      "type": "string",
      "format": "date-time"
    },
    "staffs": {
      "type": "array",
      "items": {
        "$ref": "staff.json"
      }
    }
  },
  "required": ["passaggi", "staffs"]
}
